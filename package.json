{
  "name": "nuxt-timings",
  "version": "0.1.0",
  "description": "Timings integration for Nuxt",
  "keywords": [
    "nuxt",
    "module",
    "nuxt-module",
    "timings",
    "performance"
  ],
  "repository": "danielroe/nuxt-timings-module",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "siroc build && mkdist --src src/templates --dist dist/templates",
    "dev": "nuxt example",
    "lint": "eslint --ext .js,.ts,.vue .",
